                \global\let\@midlist\@empty
                \@combinefloats
                \eb@ifvoid\footins{}
                  {
                    \setbox\@outputbox\vbox
                      {
                        \boxmaxdepth\@maxdepth
                        \unvbox\@outputbox
                        \vfill\relax
                        \vskip\skip\footins
                        \color@begingroup
                        \normalcolor\footnoterule
                        \unvbox\footins
                        \color@endgroup
                      }
                  }
